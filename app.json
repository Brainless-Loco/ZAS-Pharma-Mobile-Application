{
  "expo": {
    "name": "ZAS-MediGuide-Application",
    "slug": "ZAS-MediGuide-Application",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/zas-mediguide-logo.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/zas-mediguide-logo.png",
        "backgroundColor": "#DAE4F5"
      },
      "package": "com.brainless_loco.ZASMediGuideApp"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/zas-mediguide-logo.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/zas-mediguide-logo.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#DAE4F5"
        }
      ],
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "de240050-1305-48a5-ba42-8a97459eeb0f"
      }
    }
  }
}
