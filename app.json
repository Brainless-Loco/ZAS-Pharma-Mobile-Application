{
  "expo": {
    "name": "ZAS-Pharma-Application",
    "slug": "ZAS-Pharma-Application",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#DAE4F5"
      },
      "package": "com.brainless_loco.ZASPharmaApplication"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/zas-group-logo.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#DAE4F5"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "1b7e3d25-6eec-44df-a941-6a894003ea7f"
      }
    }
  }
}
